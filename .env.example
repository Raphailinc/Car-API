DATABASE_URL=postgresql://car_user:car_password@db:5432/car_api
PORT=8000
# Comma-separated CORS origins (defaults allow localhost dev)
CORS_ORIGINS=http://localhost:8000,http://localhost:5173
# Optional stricter list for destructive methods (DELETE); falls back to CORS_ORIGINS if empty
CORS_TRUSTED_ORIGINS=
# Admin token to enable /cars/delete-all (if empty the endpoint is disabled)
ADMIN_TOKEN=
